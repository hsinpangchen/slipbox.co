<section id="faq" class="faq">
    <div class="section-container">
        <p class="section-subtitle"><%= t('slip.faq.subtitle') %></p>
        <h3 class="section-title"><%= t('slip.faq.title') %></h3>
        <div class="faq-list">
            <div class="faq-item">
                <h4><%= t('slip.faq.items.offline.question') %></h4>
                <p><%= t('slip.faq.items.offline.answer') %></p>
            </div>
            <div class="faq-item">
                <h4><%= t('slip.faq.items.print_limit.question') %></h4>
                <p><%= t('slip.faq.items.print_limit.answer') %></p>
            </div>
            <div class="faq-item">
                <h4><%= t('slip.faq.items.subscription.question') %></h4>
                <p><%= t('slip.faq.items.subscription.answer') %></p>
            </div>
        </div>
    </div>
</section>
