---
title_key: tutorial.pages.index.title
description_key: tutorial.pages.index.description
layout: tutorial
---

<section class="guide-page">
    <div class="guide-container">
        <header class="guide-header">
            <p class="section-subtitle"><%= t('tutorial.index.subtitle') %></p>
            <h1 class="guide-title"><%= t('tutorial.index.title') %></h1>
            <p class="guide-lede"><%= t('tutorial.index.lede') %></p>
        </header>

        <div class="tutorials-grid guide-grid">
            <a class="tutorial-card" href="<%= localized_path('/slip/tutorial/menu-import-with-chatgpt/') %>">
                <div class="tutorial-card-meta">
                    <span class="tutorial-tag"><%= t('tutorial.index.cards.menu_import.duration') %></span>
                </div>
                <h4><%= t('tutorial.index.cards.menu_import.title') %></h4>
                <p><%= t('tutorial.index.cards.menu_import.description') %></p>
                <span class="tutorial-cta"><%= t('tutorial.index.cards.menu_import.cta') %></span>
            </a>

            <div class="tutorial-card is-disabled">
                <div class="tutorial-card-meta">
                    <span class="tutorial-tag tutorial-tag--muted"><%= t('tutorial.index.cards.printer.status') %></span>
                </div>
                <h4><%= t('tutorial.index.cards.printer.title') %></h4>
                <p><%= t('tutorial.index.cards.printer.description') %></p>
                <span class="tutorial-cta"><%= t('tutorial.index.cards.printer.cta') %></span>
            </div>

            <div class="tutorial-card is-disabled">
                <div class="tutorial-card-meta">
                    <span class="tutorial-tag tutorial-tag--muted"><%= t('tutorial.index.cards.opening.status') %></span>
                </div>
                <h4><%= t('tutorial.index.cards.opening.title') %></h4>
                <p><%= t('tutorial.index.cards.opening.description') %></p>
                <span class="tutorial-cta"><%= t('tutorial.index.cards.opening.cta') %></span>
            </div>

            <div class="tutorial-card is-disabled">
                <div class="tutorial-card-meta">
                    <span class="tutorial-tag tutorial-tag--muted"><%= t('tutorial.index.cards.catalog.status') %></span>
                </div>
                <h4><%= t('tutorial.index.cards.catalog.title') %></h4>
                <p><%= t('tutorial.index.cards.catalog.description') %></p>
                <span class="tutorial-cta"><%= t('tutorial.index.cards.catalog.cta') %></span>
            </div>

            <div class="tutorial-card is-disabled">
                <div class="tutorial-card-meta">
                    <span class="tutorial-tag tutorial-tag--muted"><%= t('tutorial.index.cards.shift.status') %></span>
                </div>
                <h4><%= t('tutorial.index.cards.shift.title') %></h4>
                <p><%= t('tutorial.index.cards.shift.description') %></p>
                <span class="tutorial-cta"><%= t('tutorial.index.cards.shift.cta') %></span>
            </div>
        </div>

        <div class="guide-help">
            <p><%= t('tutorial.index.help.title') %></p>
            <div class="guide-help-actions">
                <a class="btn btn-secondary" href="https://tally.so/r/wAMX1l" target="_blank"><%= t('tutorial.index.help.contact') %></a>
                <a class="btn btn-primary" href="<%= localized_path('/slip/#faq') %>"><%= t('tutorial.index.help.faq') %></a>
            </div>
        </div>
    </div>
</section>
