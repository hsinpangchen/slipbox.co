---
title_key: tutorial.pages.menu_import.title
description_key: tutorial.pages.menu_import.description
layout: tutorial
---

<section class="menu-import-page">
    <header class="menu-import-header">
        <h1><%= t('tutorial.menu_import.title') %></h1>
        <p class="menu-import-subtitle"><%= t('tutorial.menu_import.subtitle') %></p>
    </header>

    <section class="menu-import-overview">
        <p><%= t('tutorial.menu_import.overview_html') %></p>
        <p><%= t('tutorial.menu_import.suitable_title') %></p>
        <ul>
            <li><%= t('tutorial.menu_import.suitable_items.first') %></li>
            <li><%= t('tutorial.menu_import.suitable_items.second') %></li>
        </ul>
    </section>

    <section class="menu-import-steps">
        <h2><%= t('tutorial.menu_import.steps.title') %></h2>
        <ol>
            <li>
                <h3><%= t('tutorial.menu_import.steps.step1.title') %></h3>
                <p><%= t('tutorial.menu_import.steps.step1.body') %></p>
            </li>
            <li>
                <h3><%= t('tutorial.menu_import.steps.step2.title') %></h3>
                <ul>
                    <li><%= t('tutorial.menu_import.steps.step2.items.first') %></li>
                    <li><%= t('tutorial.menu_import.steps.step2.items.second') %></li>
                    <li><%= t('tutorial.menu_import.steps.step2.items.third') %></li>
                </ul>
            </li>
            <li>
                <h3><%= t('tutorial.menu_import.steps.step3.title') %></h3>
                <p><%= t('tutorial.menu_import.steps.step3.body') %></p>
                <div class="menu-import-prompt-actions">
                    <button class="menu-import-copy-btn" type="button" data-copy-target="#menu-import-prompt" data-copy-label="<%= t('common.copy') %>" data-copied-label="<%= t('common.copied') %>" aria-label="<%= t('tutorial.menu_import.steps.step3.copy_label') %>"><%= t('common.copy') %></button>
                </div>
                <pre class="menu-import-prompt" id="menu-import-prompt"><code><%= t('tutorial.menu_import.prompt') %></code></pre>
            </li>
            <li>
                <h3><%= t('tutorial.menu_import.steps.step4.title_html') %></h3>
                <p><%= t('tutorial.menu_import.steps.step4.body_html') %></p>
                <p class="menu-import-preview-label"><%= t('tutorial.menu_import.steps.step4.preview_label') %></p>
                <div class="menu-import-prompt-actions">
                    <button class="menu-import-copy-btn" type="button" data-copy-target="#menu-import-json-preview" data-copy-label="<%= t('common.copy') %>" data-copied-label="<%= t('common.copied') %>" aria-label="<%= t('tutorial.menu_import.steps.step4.copy_label') %>"><%= t('common.copy') %></button>
                </div>
                <pre class="menu-import-prompt menu-import-json-preview" id="menu-import-json-preview"><code><%= t('tutorial.menu_import.json_preview') %></code></pre>
            </li>
            <li>
                <h3><%= t('tutorial.menu_import.steps.step5.title') %></h3>
                <p><%= t('tutorial.menu_import.steps.step5.body') %></p>
            </li>
        </ol>
    </section>

    <section class="menu-import-notes">
        <h2><%= t('tutorial.menu_import.notes.title') %></h2>
        <ul>
            <li><%= t('tutorial.menu_import.notes.items.first') %></li>
            <li><%= t('tutorial.menu_import.notes.items.second') %></li>
            <li><%= t('tutorial.menu_import.notes.items.third') %></li>
        </ul>
    </section>

    <section class="menu-import-disclaimer">
        <h2><%= t('tutorial.menu_import.disclaimer.title') %></h2>
        <p><%= t('tutorial.menu_import.disclaimer.body') %></p>
    </section>
</section>
